{
  "name": "xbl-redis-desktop",
  "version": "1.0.0",
  "type": "module",
  "description": "A Windows Redis desktop tool with type-safe operations",
  "main": "electron/main.cjs",
  "author": {
    "name": "hukun",
    "email": "1181929830@qq.com"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"npm run dev\" \"cross-env NODE_ENV=development electron .\" --names \"Vite,Electron\"",
    "electron:build": "npm run build && node scripts/copy-electron-to-dist.js && electron-builder --project=dist -p never",
    "electron:build:win": "npm run build && node scripts/copy-electron-to-dist.js && electron-builder --project=dist -w -p never",
    "electron:build:mac": "npm run build && node scripts/copy-electron-to-dist.js && electron-builder --project=dist -m -p never",
    "preview": "vite preview"
  },
  "dependencies": {
    "ioredis": "^5.4.1",
    "java-object-serialization": "^0.1.2",
    "lucide-react": "^0.400.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-electron": "^0.29.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.xbl.redis-desktop",
    "productName": "Xbl Redis Desktop",
    "artifactName": "${productName}-${os}-${version}.${ext}",
    "copyright": "Copyright Â© 2024 hukun",
    "files": [
      "**/*",
      "!**/*.map",
      "!**/*.ts",
      "!**/*.spec.*",
      "!**/*.test.*",
      "!src/**/*",
      "!.git/**/*",
      "!.idea/**/*",
      "!*.md",
      "!*.yaml",
      "!.gitignore",
      "!scripts/**/*",
      "!screenshots/**/*"
    ],
    "asar": true,
    "asarUnpack": [
      "node_modules/**/{test,tests,src,example,examples}/**",
      "node_modules/**/.gitignore"
    ],
    "directories": {
      "output": "build-apps"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "signAndEditExecutable": false,
      "signDlls": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "perMachine": false,
      "deleteAppDataOnUninstall": false
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "public.app-category.developer-tools",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}